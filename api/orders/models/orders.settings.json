{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "name": "Orders",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "order_id": {
      "type": "integer",
      "required": true
    },
    "order_list": {
      "type": "component",
      "repeatable": true,
      "component": "order.order-list"
    },
    "billing": {
      "type": "component",
      "repeatable": false,
      "component": "order.billing"
    },
    "order_status": {
      "type": "enumeration",
      "enum": [
        "on_hold",
        "awaitting_payment",
        "awaiting_shipment",
        "completed",
        "shipped",
        "cancelled",
        "refunded"
      ]
    },
    "total_bill": {
      "type": "float"
    },
    "payment_method": {
      "type": "string"
    }
  }
}
